  title = {Managing the Middle: {{A}} Shift in Conservation Priorities Based on the Global Human Modification Gradient},
  author = {Kennedy, Christina M. and Oakleaf, James R. and Theobald, David M. and Baruch-Mordo, Sharon and Kiesecker, Joseph},
