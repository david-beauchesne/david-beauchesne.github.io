@article{fernandes2018,
  type = {Article},
  title = {Identifying Conservation Priority Areas to Inform Maritime Spatial Planning: {{A}} New Approach},
  author = {Fernandes, Maria da Luz and Quintela, Adriano and Alves, Fatima L.},
