  title = {Perspectives from Landscape Ecology Can Improve Environmental Impact Assessment},
  author = {Harker, Karly J. and Arnold, Lauren and Sutherland, Ira J. and Gergel, Sarah E.},
  date = {2021-03-18},
  journaltitle = {FACETS},
