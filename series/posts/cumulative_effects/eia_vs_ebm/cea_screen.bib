@article{smit2024,
  type = {Article},
  title = {Groundtruthing Cumulative Impact Assessments with Biodiversity Data: {{Testing}} Indicators and Methods for Marine Ecosystem Condition Assessments in {{South Africa}}},
  author = {Smit, Kaylee P. and Sink, Kerry J. and Shannon, Lynne J. and Bernard, Anthony T. F. and Lombard, Amanda T.},
  date = {2024-02},
  journaltitle = {AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS},
  volume = {34},
  number = {e4096},
  publisher = {WILEY},
  location = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
  issn = {1052-7613},
  doi = {10.1002/aqc.4096},
  abstract = {Accurate assessments of ecosystem condition are needed to inform management processes such as systematic conservation planning and protected area expansion, marine spatial planning and other effective area-based conservation measures. Yet measuring the condition of dynamic and often largely inaccessible marine ecosystems is extremely difficult and presents a global challenge. Broad-scale cumulative impact assessments have been used in South Africa as a proxy for marine ecosystem condition in national assessments and reporting because the data are more readily available, and knowledge exists on the impacts of various human activities (or pressures) on marine ecosystems. While useful for national reporting, assessments based on in situ ecological data are also needed at finer scales relevant to management. Applying the HELCOM BEAT approach, this study used in situ ecological data to identify potential methods and indicators that can be applied in South Africa to conduct integrated ecosystem assessments and to assess the suitability of current cumulative impact assessments as a proxy for condition. Aggregated fish indicators demonstrated greater sensitivity to varying human pressures compared with benthic community indicators, with responsiveness differing by location. Notably, site-level differences in empirical condition emerged that broad-scale cumulative assessments often missed. The study thus highlighted the varying suitability of cumulative impact scores as proxies for condition, indicating their reliability for fish communities. However, this is also dependent on the pressure types included, the biotic indicators used and the area of focus. Conservation decisions based solely on cumulative impact scores could misrepresent the actual condition of the environment, which makes it difficult to make management decisions, especially at finer scales. As such, conservation initiatives need to be regionally adapted to effectively address the unique pressures and conditions of each area and should be tailored to account for fine-scale variability, instead of adopting a one-size-fits-all approach.},
  affiliation = {Smit, KP (Corresponding Author), Univ Cape Town, Dept Biol Sci, Cape Town, South Africa. Smit, Kaylee P.; Shannon, Lynne J., Univ Cape Town, Dept Biol Sci, Cape Town, South Africa. Smit, Kaylee P.; Sink, Kerry J., South African Natl Biodivers Inst, Kirstenbosch, Cape Town, South Africa. Smit, Kaylee P.; Bernard, Anthony T. F., South African Inst Aquat Biodivers, Makhanda, South Africa. Smit, Kaylee P.; Sink, Kerry J.; Lombard, Amanda T., Nelson Mandela Univ, Inst Coastal \& Marine Res, Gqeberha, South Africa. Bernard, Anthony T. F., Rhodes Univ, Dept Zool \& Entomol, Makhanda, South Africa.},
  affiliations = {University of Cape Town; South African National Biodiversity Institute; National Research Foundation - South Africa; South African Institute for Aquatic Biodiversity; Nelson Mandela University; Rhodes University},
  author-email = {kayles.smit@gmail.com},
  da = {2024-04-23},
  doc-delivery-number = {IE2T3},
  eissn = {1099-0755},
  funding-acknowledgement = {South African Department of Science and Innovation (DSI) National Research Foundation; Nelson Mandela University (NMU, South Africa), South African National Biodiversity Institute (SANBI) - DSI/NRF South African Research Chairs Initiative [98574]; European Union's Horizon 2020 Research and Innovation Program [8862428]; African Coelacanth Ecosystem Project of the National Research Foundation (NRF) [129216]; [:101723]},
  funding-text = {The authors acknowledge funding from the South African Department of Science and Innovation (DSI) National Research Foundation (DSI/NRF, South Africa ID:101723), Nelson Mandela University (NMU, South Africa), South African National Biodiversity Institute (SANBI) and DSI/NRF South African Institute of Aquatic Biodiversity (SAIAB). Amanda Lombard is funded by the DSI/NRF South African Research Chairs Initiative (Grant 98574). Lynne Shannon and Kerry Sink acknowledge the European Union's Horizon 2020 Research and Innovation Program under Grant Agreement No. 8862428 (Mission Atlantic). Kerry Sink acknowledges the Deep Connections project funded through the African Coelacanth Ecosystem Project of the National Research Foundation (NRF Grant 129216).},
  issue = {2},
  journal-iso = {Aquat. Conserv.-Mar. Freshw. Ecosyst.},
  keywords-plus = {BENTHIC ECOLOGICAL STATUS; ENVIRONMENTAL STATUS; ASSESSMENT FRAMEWORK; MULTIMETRIC INDEXES; EXPERT JUDGMENT; FISH; HEALTH; COMMUNITY; COASTAL; REEFS},
