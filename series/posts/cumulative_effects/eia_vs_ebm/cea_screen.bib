  funding-acknowledgement = {Pacific Institute for Climate Solutions; Natural Sciences and Engineering Research Council of Canada; Coordenacao de Aperfeicoamento de Pessoal de Nivel Superior; Social Sciences and Humanities Research Council of Canada; Conselho Nacional de Desenvolvimento Cientifico e Tecnologico; National Science Foundation; Vanier Canada Graduate Scholarship; Science Without Borders Program; WWF-Canada; Government of Canada's Summer Jobs Program},
