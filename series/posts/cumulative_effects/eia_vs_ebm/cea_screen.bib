  web.of.science.index = {Science Citation Index Expanded (SCI-EXPANDED)},
  keywords = {canada,Collaboration,Cross-cultural knowledge,Cumulative effects,eia,freshwater,indigenous,management,method,ontario,problem,process,roles,Skimmed,social,solution,Water quality}
}

@article{ho2018,
