  keywords-plus = {TIDE-GAUGE RECORDS; ATMOSPHERIC CARBON-DIOXIDE; EAST-AFRICAN REGION; RELATIVE SEA LEVELS; RAIN FORESTS; CORAL REEFS; CLIMATE; TEMPERATURE; DEFORESTATION; ECOSYSTEMS},
  langid = {english},
  number-of-cited-references = {75},
