  title = {Advancing Global Ecological Modeling Capabilities to Simulate Future Trajectories of Change in Marine Ecosystems},
  author = {Coll, Marta and Steenbeek, Jeroen and Pennino, Maria Grazia and Buszowski, Joe and Kaschner, Kristin and Lotze, Heike K. and Rousseau, Yannick and Tittensor, Derek P. and Walters, Carl and Watson, Reg A. and Christensen, Villy},
  date = {2020-10-16},
