@article{sutherland2022,
  type = {Article},
  title = {A Pragmatic Approach for Developing Landbase Cumulative Effects Assessments with Aggregated Impacts Crossing Multiple Ecological Values},
  author = {Sutherland, Glenn D. and Smith, Jason and Waterhouse, F. Louise and Saunders, Sari C. and Paige, Kathy},
