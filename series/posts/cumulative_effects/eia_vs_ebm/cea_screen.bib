  journal-iso = {Extr. Ind. Soc.},
  keywords-plus = {CUMULATIVE EFFECTS ASSESSMENT; EXTRACTIVE INDUSTRIES; HUSBANDRY; EXPERIENCES; POPULATION; GOVERNANCE; PROJECTS; FINNMARK; SWEDEN; POLICY},
