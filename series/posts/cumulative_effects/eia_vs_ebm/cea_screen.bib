  title = {A Roadmap towards Quantitative Cumulative Impact Assessments: {{Every}} Step of the Way},
  author = {Piet, Gerjan J. and Tamis, Jacqueline E. and Volwater, Joey and family=Vries, given=Pepijn, prefix=de, useprefix=true and family=Wal, given=Jan Tjalling, prefix=van der, useprefix=true and Jongbloed, Ruud H.},
  date = {2021-08-25},
