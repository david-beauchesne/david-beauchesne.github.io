  eissn = {1365-2664},
  funding-acknowledgement = {BHP},
  funding-text = {This research was supported by research funding from BHP. We thank for George Watson and Tanya Carroll for supporting this research and providing relevant data. We also thank Chris Ware and Danial Stratford for providing useful feedback on an earlier version of this manuscript.},
  journal-iso = {J. Appl. Ecol.},
