  journal-iso = {Environ. Sci. Technol.},
  keywords-plus = {SODIUM-IODIDE SYMPORTER; AMMONIUM-PERCHLORATE; THYROID-FUNCTION; XENOPUS-LAEVIS; DOSE-RESPONSE; MECHANISM; TOXICITY; ECOTOXICOLOGY; METAMORPHOSIS; CONTAMINATION},
