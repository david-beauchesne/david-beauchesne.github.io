  research-areas = {Environmental Sciences \& Ecology},
  researcherid-numbers = {Strimbu, Bogdan/A-8669-2008 Strimbu, Bogdan/GZG-1790-2022 Strimbu, Bogdan/A-5045-2014 Innes, John/E-4355-2013},
