  journal-iso = {Environ. Manage.},
  keywords-plus = {CUMULATIVE EFFECTS ASSESSMENT; HABITAT FRAGMENTATION; CIRCUIT-THEORY; CLIMATE-CHANGE; SAMPLE-SIZE; CONSERVATION; EXTINCTION; DISPERSAL; IMPACTS; PERFORMANCE},
  langid = {english},
