@article{fernandes2017,
  type = {Article},
  title = {How Does the Cumulative Impacts Approach Support {{Maritime Spatial Planning}}?},
  author = {Fernandes, Maria da Luz and Esteves, Tanya C. and Oliveira, Eduardo R. and Alves, Fatima L.},
