@article{rooney2023,
  type = {Article},
  title = {Fuzzy Cognitive Mapping as a Tool to Assess the Relative Cumulative Effects of Environmental Stressors on an {{Arctic}} Seabird Population to Identify Conservation Action and Research Priorities},
  author = {Rooney, Rebecca C. and Daniel, Jody and Mallory, Mark and Hedd, April and Ives, Jess and Gilchrist, Grant and Gjerdrum, Carina and Robertson, Greg and Ronconi, Rob and Wilcox, Kirsten and Wong, Sarah and Provencher, Jennifer},
  date = {2023-04},
