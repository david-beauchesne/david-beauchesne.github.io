@article{harford2022,
  type = {Article},
  title = {Resolving Ecosystem Complexity in Ecological Risk Assessment for Mine Site Rehabilitation},
  author = {Harford, Andrew J. and Bartolo, Renee E. and Humphrey, Chris L. and Nicholson, Jaylen D. and Richardson, Darren L. and Rissik, David and Iles, Michelle and Dambacher, Jeffrey M.},
  date = {2022-10-01},
  journaltitle = {JOURNAL OF ENVIRONMENTAL MANAGEMENT},
