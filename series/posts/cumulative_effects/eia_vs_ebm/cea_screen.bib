  journal-iso = {Environ. Impact Assess. Rev.},
  keywords-plus = {ECOLOGICAL RISK-ASSESSMENT; CUMULATIVE EFFECTS ASSESSMENT; ADVERSE OUTCOME PATHWAYS; AQUATIC ECOSYSTEMS; CONCEPTUAL MODELS; COMMUNITY ECOLOGY; BAYESIAN NETWORK; FRAMEWORK; MACROINVERTEBRATES; SUPPORT},
