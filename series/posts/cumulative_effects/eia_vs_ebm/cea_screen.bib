@article{sergeant2022,
  type = {Review},
  title = {Risks of Mining to Salmonid-Bearing Watersheds},
  author = {Sergeant, Christopher J. and Sexton, Erin K. and Moore, Jonathan W. and Westwood, Alana R. and Nagorski, Sonia A. and Ebersole, Joseph L. and Chambers, David M. and O'Neal, Sarah L. and Malison, Rachel L. and Hauer, F. Richard and Whited, Diane C. and Weitz, Jill and Caldwell, Jackie and Capito, Marissa and Connor, Mark and Frissell, Christopher A. and Knox, Greg and Lowery, Erin D. and Macnair, Randal and Marlatt, Vicki and McIntyre, Jenifer K. and McPhee, V, Megan and Skuce, Nikki},
  date = {2022-07-01},
  journaltitle = {SCIENCE ADVANCES},
  volume = {8},
  number = {eabn0929},
  publisher = {AMER ASSOC ADVANCEMENT SCIENCE},
  location = {1200 NEW YORK AVE, NW, WASHINGTON, DC 20005 USA},
  issn = {2375-2548},
  doi = {10.1126/sciadv.abn0929},
  abstract = {Mining provides resources for people but can pose risks to ecosystems that support cultural keystone species. Our synthesis reviews relevant aspects of mining operations, describes the ecology of salmonid-bearing watersheds in northwestern North America, and compiles the impacts of metal and coal extraction on salmonids and their habitat. We conservatively estimate that this region encompasses nearly 4000 past producing mines, with present-day operations ranging from small placer sites to massive open-pit projects that annually mine more than 118 million metric tons of earth. Despite impact assessments that are intended to evaluate risk and inform mitigation, mines continue to harm salmonid-bearing watersheds via pathways such as toxic contaminants, stream channel burial, and flow regime alteration. To better maintain watershed processes that benefit salmonids, we highlight key windows during the mining governance life cycle for science to guide policy by more accurately accounting for stressor complexity, cumulative effects, and future environmental change.},
  affiliation = {Sergeant, CJ (Corresponding Author), Univ Montana, Flathead Lake Biol Stn, Polson, MT 59860 USA. Sergeant, CJ (Corresponding Author), Univ Alaska Fairbanks, Coll Fisheries \& Ocean Sci, Juneau, AK 99801 USA. Sergeant, Christopher J.; Sexton, Erin K.; Malison, Rachel L.; Hauer, F. Richard; Whited, Diane C.; Frissell, Christopher A., Univ Montana, Flathead Lake Biol Stn, Polson, MT 59860 USA. Sergeant, Christopher J.; McPhee, Megan, V, Univ Alaska Fairbanks, Coll Fisheries \& Ocean Sci, Juneau, AK 99801 USA. Moore, Jonathan W., Simon Fraser Univ, Earth2Ocean Res Grp, Burnaby, BC V5A 1S6, Canada. Westwood, Alana R., Dalhousie Univ, Sch Resource \& Environm Studies, Halifax, NS B3H 4R2, Canada. Nagorski, Sonia A., Univ Alaska Southeast, Environm Sci Program, Juneau, AK 99801 USA. Ebersole, Joseph L., US EPA, Corvallis, OR 97333 USA. Chambers, David M., Ctr Sci Publ Participat, Bozeman, MT 59715 USA. O'Neal, Sarah L., Univ Washington, Sch Aquat \& Fishery Sci, Seattle, WA 98105 USA. Weitz, Jill, Salmon Borders, Juneau, AK 99801 USA. Caldwell, Jackie; Connor, Mark, Taku River Tlingit First Nat, Lands Resources \& Fisheries, Atlin, BC V0W 1A0, Canada. Frissell, Christopher A., Salish Kootenai Coll, Dept Hydrol, Pablo, MT 59855 USA. Knox, Greg, SkeenaWild Conservat Trust, Terrace, BC V8G 1M9, Canada. Lowery, Erin D., Seattle City Light, Environm Land \& Licensing Business Unit, Seattle, WA 98104 USA. Macnair, Randal, Wildsight, Kimberley, BC V1A 1Z6, Canada. Marlatt, Vicki, Simon Fraser Univ, Dept Biol Sci, Burnaby, BC V5A 1S6, Canada. McIntyre, Jenifer K., Washington State Univ, Puyallup Res \& Extens Ctr, Sch Environm, Puyallup, WA 98371 USA. Skuce, Nikki, Northern Confluence Initiat, Smithers, BC V0J 2N0, Canada.},
