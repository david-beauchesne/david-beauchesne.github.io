  journal-iso = {Sci. Total Environ.},
  keywords-plus = {QUALITATIVE RISK-ASSESSMENT; PRACTICAL IMPLEMENTATION; RESPONSE RELATIONSHIPS; MARINE; MANAGEMENT; VULNERABILITY; FISHERIES; ECOSYSTEMS; STRESSORS; FRAMEWORK},
