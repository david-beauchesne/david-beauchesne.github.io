  title = {An Effective Set of Principles for Practical Implementation of Marine Cumulative Effects Assessment},
  author = {Judd, A. D. and Backhaus, T. and Goodsir, F.},
  date = {2015-12},
  journaltitle = {ENVIRONMENTAL SCIENCE \& POLICY},
  volume = {54},
