@article{almpanidou2022,
  type = {Article},
  title = {Foraging Grounds of Adult Loggerhead Sea Turtles across the {{Mediterranean Sea}}: Key Sites and Hotspots of Risk},
  author = {Almpanidou, Vasiliki and Tsapalou, Vasiliki and Chatzimentor, Anastasia and Cardona, Luis and Claro, Francoise and Hostetter, Patrice and Kaska, Yakup and Liu, Wenhua and Mansui, Jeremy and Miliou, Anastasia and Pietroluongo, Guido and Sacchi, Jacques and Sezgin, Cisem and Sozbilen, Dogan and Mazaris, Antonios D.},
  date = {2022-01},
  journaltitle = {BIODIVERSITY AND CONSERVATION},
