@article{batista2014,
  title = {Assessment of Cumulative Human Pressures on a Coastal Area: {{Integrating}} Information for {{MPA}} Planning and Management},
  shorttitle = {Assessment of Cumulative Human Pressures on a Coastal Area},
  author = {Batista, Marisa I. and Henriques, Sofia and Pais, Miguel P. and Cabral, Henrique N.},
  date = {2014-12},
  journaltitle = {Ocean \& Coastal Management},
  shortjournal = {Ocean \& Coastal Management},
  volume = {102, Part A},
  pages = {248--257},
  issn = {0964-5691},
  doi = {10.1016/j.ocecoaman.2014.09.020},
  url = {http://www.sciencedirect.com/science/article/pii/S0964569114003019},
  urldate = {2015-01-05},
  abstract = {As recently reinforced in the EU Marine Strategy Framework Directive (MSFD), knowledge on the location and intensity of human impacts on marine ecosystems is critical for effective marine management and conservation. Human interaction with ecosystems has to be accounted for in order to efficiently implement marine management strategies. In the present study, the main human activities occurring along the mainland Portuguese coast were identified and mapped. The cumulative impact of these activities was calculated in order to assess impacts in different zones, namely in Marine Protected Areas (MPA) and their boundaries. Higher impact values were obtained near the coast, where all the analysed MPAs are located. Furthermore, most MPAs are sorrounded by areas with very high impacts, near the largest urban settlements and the most industrialized coastal sections. These results are the first assessment of cumulative human pressures in this study area as a whole (and with this level of resolution) and might be of great usefulness to overcome the current challenges of sustainable management in marine ecosystems. Knowledge provided by this study strengthens the need for a more integrative approach to design and manage MPAs and can be useful to support the requirements of the MSFD. The approach here developed is also a powerful tool to apply in several contexts of sustainable marine management and can be developed in any geographic area.},
  keywords = {case study,cumulative impact assessment,Cumulative impact assessment,ebm,Environmental management,Environmental Management,Geographic information system,geospatial,marine,marine protected areas,Marine protected areas,Marine strategy framework directive,Marine Strategy Framework Directive,portugal,Skimmed},
  file = {/Users/davidbeauchesne/Zotero/storage/U8ITBHVI/S0964569114003019.html}
}

@article{beauchesne2020,
  title = {Characterizing {{Exposure}} to and {{Sharing Knowledge}} of {{Drivers}} of {{Environmental Change}} in the {{St}}. {{Lawrence System}} in {{Canada}}},
  author = {Beauchesne, David and Daigle, Rémi M. and Vissault, Steve and Gravel, Dominique and Bastien, Andréane and Bélanger, Simon and Bernatchez, Pascal and Blais, Marjolaine and Bourdages, Hugo and Chion, Clément and Galbraith, Peter S. and Halpern, Benjamin S. and Lavoie, Camille and McKindsey, Christopher W. and Mucci, Alfonso and Pineault, Simon and Starr, Michel and Ste-Marie, Anne-Sophie and Archambault, Philippe},
  date = {2020},
  journaltitle = {Frontiers in Marine Science},
  shortjournal = {Front. Mar. Sci.},
  volume = {7},
  publisher = {Frontiers},
  issn = {2296-7745},
  doi = {10.3389/fmars.2020.00383},
  url = {https://www.frontiersin.org/articles/10.3389/fmars.2020.00383/full?&utm_source=Email_to_authors_&utm_medium=Email&utm_content=T1_11.5e1_author&utm_campaign=Email_publication&field=&journalName=Frontiers_in_Marine_Science&id=469411},
  urldate = {2020-06-25},
  abstract = {The St. Lawrence is a vast and complex socio-ecological system providing a wealth of services sustaining numerous economic sectors. These ecosystems are subject to significant human pressures that overlap and potentially interact with climate driven environmental changes. Our objective in this paper is to systematically characterize the distribution and intensity of drivers in the St. Lawrence System. To do so, we launch eDrivers, an open knowledge platform gathering experts committed to structuring, standardizing and sharing knowledge on drivers in support of science and management. We gathered data on 22 coastal, climate, fisheries and marine traffic drivers through collaborations, existing environmental initiatives and open data portals. We show that few areas of the St. Lawrence are free of cumulative exposure. The Estuary, the Anticosti Gyre and coastal areas are particularly exposed, especially in the vicinity of urban centers. We identified 6 areas of distinct cumulative exposure regime that show that certain drivers typically co-occur in different regions of the St. Lawrence and that coastal areas are exposed to all driver types. Of particular concern are two threat complexes capturing most exposure hotspots that show the convergence of contrasting exposure regimes at the head of the Laurentian Channel. eDrivers was built on a series of guiding principles upholding existing data management and open science standards. We therefore expect it to evolve through time to address knowledge gaps and refine current driver layers. Ultimately, we believe that eDrivers represents a much needed solution that could radically influence broad scale research and management practices by increasing knowledge accessibility and interoperability.},
  langid = {english},
  keywords = {canada,cumulative exposure,data,ebm,Environmental drivers,geospatial,global change,marine,multiple stressors,ocean observing systems,Skimmed,solution,St. Lawrence,threat complex},
  file = {/Users/davidbeauchesne/Zotero/storage/FSP544TS/Beauchesne et al. - 2020 - Characterizing Exposure to and Sharing Knowledge o.pdf}
}

@article{bedford1999,
  type = {Article},
  title = {Cumulative Effects on Wetland Landscapes: {{Links}} to Wetland Restoration in the {{United States}} and Southern {{Canada}}},
  author = {family=Bedford, given=BL, given-i=BL},
  date = {1999-12},
  journaltitle = {WETLANDS},
  volume = {19},
  number = {4},
  pages = {775--788},
  publisher = {SOC WETLAND SCIENTISTS},
  location = {810 E TENTH ST, P O BOX 1897, LAWRENCE, KS 66044 USA},
  issn = {0277-5212},
  doi = {10.1007/BF03161784},
  abstract = {The cumulative effects of human actions on wetland ecosystems motivate current efforts at wetland restoration. They also have created in part the context within which restorations are undertaken. Using modern hydrogeological understanding of wetland-landscape linkages, I argue that restorations should begin with a cumulative impact analysis for the entire region in which the restoration is proposed. The analysis, however, should not focus merely on number of hectares of wetlands lost or degraded. It should be based on the concept of templates for wetland development. These templates are the diversity of settings created in specific landscapes by the complex interactions of hydrogeologic factors and climate. They control key hydrologic variables and hydrologically influenced chemical variables that cause specific wetland types to form and to be maintained through time. They also determine in large part the biogeochemical cycling characteristics specific to different types of wetlands. They thus account for both the biological and functional diversity of wetlands. A cumulative impact assessment for restoration purposes should identify the kinds, numbers, relative abundances, and spatial distribution of wetland templates in a region-both past and present. These past and present profiles of the wetland landscape can be used to make decisions regarding the type and location of restorations. Matching type and location to the appropriate hydrogeologic setting will maximize the probability of success for individual projects. Regional wetland diversity can be restored if individual restoration decisions about wetland type and location are made in light of the diversity of templates in past and present regional profiles.},
  affiliation = {Bedford, BL (Corresponding Author), Cornell Univ, Dept Nat Resources, Fernow Hall, Ithaca, NY 14850 USA. Cornell Univ, Dept Nat Resources, Ithaca, NY 14850 USA.},
  affiliations = {Cornell University},
  da = {2024-04-23},
  doc-delivery-number = {270GC},
  journal-iso = {Wetlands},
  keywords-plus = {LAKE-AGASSIZ PEATLANDS; PLANT-SPECIES RICHNESS; LAND-USE; GROUNDWATER DISCHARGE; WATER-QUALITY; NORTHERN MINNESOTA; COASTAL-PLAIN; CENTRAL SPAIN; HYDROLOGY; IMPACTS},
  langid = {english},
  number-of-cited-references = {105},
  research-areas = {Environmental Sciences \& Ecology},
  times-cited = {105},
  unique-id = {WOS:000084526200006},
