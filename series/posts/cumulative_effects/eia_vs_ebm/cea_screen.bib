@article{farella2020,
  type = {Article},
  title = {Incorporating Ecosystem Services Conservation into a Scenario-Based {{MSP}} Framework: {{An Adriatic}} Case Study},
  author = {Farella, Giulio and Menegon, Stefano and Fadini, Amedeo and Depellegrin, Daniel and Manea, Elisabetta and Perini, Luisa and Barbanti, Andrea},
  date = {2020-08-01},
