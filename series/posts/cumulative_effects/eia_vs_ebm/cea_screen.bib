@article{lonsdale2020,
  title = {A Novel Approach for Cumulative Impacts Assessment for Marine Spatial Planning},
  author = {Lonsdale, Jemma-Anne and Nicholson, Rose and Judd, Adrian and Elliott, Michael and Clarke, Charlotte},
  date = {2020-04-01},
  journaltitle = {Environmental Science \& Policy},
  shortjournal = {Environmental Science \& Policy},
  volume = {106},
  pages = {125--135},
  issn = {1462-9011},
  doi = {10.1016/j.envsci.2020.01.011},
  url = {http://www.sciencedirect.com/science/article/pii/S1462901119311372},
  urldate = {2020-04-16},
  abstract = {The assessment of the cumulative impacts of multiple human activities is an increasingly important yet complex part of managing the marine environment. Despite this, as yet, there is no consistent approach to considering these in assessments and/or management (e.g. environmental impact assessment, marine spatial planning). Current and previous approaches to the assessment of cumulative impacts have limitations and shortcomings e.g. double counting of impacts or avoiding, complicating and confounding impacts, which need to be addressed to allow for pragmatic decisions to be made. Here, an innovative, practical approach to marine CIA is presented that considers the three dimensional nature of the marine environment, and the spatial and temporal effects-footprints likely to occur during the lifetime of a development. The new approach is tested within a GIS platform and shows an effective visual representation for decision makers, managers and policy makers on which to base their decisions. While showing potential, the approach indicates the need to incorporate temporal data and assessing temporal trends. Despite this limitation, the tool markedly improves on previous approaches due to the national and practical application, and the incorporation of 3D nature of pressures and receptors.},
  langid = {english},
  keywords = {case study,CEA,CIA,Cumulative,ebm,eia,Geographic Information System,geospatial,marine,Marine,method,Skimmed,solution,uk,United Kingdom},
  file = {/Users/davidbeauchesne/Zotero/storage/QR6LC7K5/Lonsdale et al. - 2020 - A novel approach for cumulative impacts assessment.pdf}
}

