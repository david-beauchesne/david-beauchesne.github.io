@article{greaves2024,
  type = {Article},
  title = {Re-Grounding Cumulative Effects Assessments in Ecological Resilience},
  author = {Greaves, Corrie and Parrott, Lael},
  date = {2024-03},
  journaltitle = {ENVIRONMENTAL IMPACT ASSESSMENT REVIEW},
  volume = {105},
  number = {107403},
  publisher = {ELSEVIER SCIENCE INC},
  location = {STE 800, 230 PARK AVE, NEW YORK, NY 10169 USA},
  issn = {0195-9255},
  doi = {10.1016/j.eiar.2023.107403},
  abstract = {Cumulative effects assessments (CEA) evolved to holistically understand and account for the impact of a spectrum of human and natural disturbances on ecosystems. Yet, the practice of CEA has struggled to overcome siloed and reductionist underpinnings common in the impact assessment arena. One way to move CEA towards more integrated approaches is by drawing on the concept of ecological resilience. Despite gaining considerable attention in other academic spheres, however, ecological resilience remains largely unexplored in CEAs. Motivated by this gap, the objective of this article is to explore how CEAs can be reimagined through an ecological resilience lens to cultivate more integrated and holistic CEA practices. We provide a brief synthesis of CEA theory and practice, highlighting where reductionist, disciplinary, and siloed approaches prevail. Then, we explore three shifts that could recast CEA through the concept of ecological resilience: (1) a shift from valued ecological components to values/identity (resilience pivots), (2) a shift from baseline assessments to ecological trajectories, and (3) a shift from management thresholds to safe operating spaces. We argue that intersecting the practice of CEA with the concept of ecological resilience offers a real opportunity to extend beyond simply being passive respondents to an incremental “death by 1000 cuts” to cultivating the conditions needed for ecological adaption and transformation along desirable pathways.},
  affiliation = {Parrott, L (Corresponding Author), ASC 451 3187 Univ Way, Kelowna, BC V1V 1V7, Canada. Greaves, Corrie; Parrott, Lael, Univ British Columbia, Dept Earth Environm \& Geog Sci, Okanagan Campus,3333 Univ Way, Kelowna, BC V1V 1V7, Canada. Greaves, Corrie; Parrott, Lael, Univ British Columbia, Inst Biodivers Resilience \& Ecosyst Serv, Okanagan Campus,3333 Univ Way, Kelowna, BC V1V 1V7, Canada. Parrott, Lael, Univ British Columbia, Dept Biol, Okanagan Campus,3333 Univ Way, Kelowna, BC V1V 1V7, Canada. Parrott, Lael, ASC 451 3187 Univ Way, Kelowna, BC V1V 1V7, Canada.},
  affiliations = {University of British Columbia; University of British Columbia; University of British Columbia},
  author-email = {lael.parrott@ubc.ca},
  da = {2024-04-10},
  doc-delivery-number = {FI8Y3},
  earlyaccessdate = {DEC 2023},
  eissn = {1873-6432},
  funding-acknowledgement = {Office of the Vice-Principal, Research and Innovation, University of British Columbia (Okanagan) [GR017933]},
  funding-text = {Financial support for this work was provided by the Office of the Vice-Principal, Research and Innovation, University of British Columbia (Okanagan) through an Eminence Grant on Watershed Ecosystems (GR017933) . This work was first shared at the 2023 International As-sociation for Impact Assessment conference themed “resilience through impact assessment and leadership”. We thank the conference attendees for many interesting and informative discussions, which were instrumental to shaping the work presented herein. We also extend particular gratitude to J. Wagner, A. Wei, J. Corbett, and two anonymous re-viewers for constructive dialogue on earlier drafts of this manuscript, as well as J. Gibbs for providing photographs used in Figure 3.},
