@article{hanna2019,
  type = {Article},
  title = {An Analysis of the State of Impact Assessment Research for Low Carbon Power Production: {{Building}} a Better Understanding of Information and Knowledge Gaps},
  author = {Hanna, Kevin and McGuigan, Erin and Noble, Bram and Parkins, John},
  date = {2019-04},
