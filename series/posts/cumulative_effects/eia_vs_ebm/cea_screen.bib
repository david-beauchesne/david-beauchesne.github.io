  title = {Framework Tool for a Rapid Cumulative Effects Assessment: Case of a Prominent Wetland in {{Myanmar}}},
  author = {Pradhan, N. and Habib, H. and Venkatappa, M. and Ebbers, T. and Duboz, R. and Shipin, O.},
  date = {2015-06},
  journaltitle = {ENVIRONMENTAL MONITORING AND ASSESSMENT},
  volume = {187},
