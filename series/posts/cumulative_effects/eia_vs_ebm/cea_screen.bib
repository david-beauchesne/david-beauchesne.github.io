@article{wyatt2021,
  type = {Article},
  title = {Integrated and Innovative Scenario Approaches for Sustainable Development Planning in {{The Bahamas}}},
  author = {Wyatt, Katherine H. and Arkema, Katie K. and Wells-Moultrie, Stacey and Silver, Jessica M. and Lashley, Brett and Thomas, Adelle and Kuiper, Jan J. and Guerry, Anne D. and Ruckelshaus, Mary},
