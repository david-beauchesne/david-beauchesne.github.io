@article{clark2022,
  title = {Risk Assessment for Marine Ecosystem-Based Management ({{EBM}})},
  author = {Clark, Dana E. and Gladstone-Gallagher, Rebecca V. and Hewitt, Judi E. and Stephenson, Fabrice and Ellis, Joanne I.},
  date = {2022},
  journaltitle = {Conservation Science and Practice},
  volume = {4},
  number = {3},
  pages = {e12636},
  issn = {2578-4854},
  doi = {10.1111/csp2.12636},
  url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/csp2.12636},
  urldate = {2022-08-22},
  abstract = {Ecosystem-based management (EBM) is a holistic way to manage the marine environment, involving partnerships between people and the recognition of ecological complexity. As we progress towards EBM, risk assessments must move beyond an evaluation of the direct impacts of a single stressor on a species or habitat. Here, we propose 12 risk assessment criteria that explicitly reflect the principles of EBM. These criteria include the need to assess risk to multiple ecosystem components and values, evaluate place and time-specific ecological complexity, evaluate recovery, accommodate different knowledge types and communicate uncertainty. Contemporary risk assessment approaches rarely meet all 12 criteria and whilst many approaches could be adapted to do so, some are more easily modified than others. Risk assessment approaches that meet our criteria have the greatest potential to support decision-making in an EBM context and thereby safeguard our marine environments and their values for future generations.},
  langid = {english},
  keywords = {Bayesian networks,coupled natural-human systems,cumulative effects assessments,decision support,ebm,ecosystem integrity,Indigenous knowledge,marine,multiple stressors,New Zealand,review,risk,risk analysis,Skimmed,solution,To read,uncertainty},
  file = {/Users/davidbeauchesne/Zotero/storage/FAEIE5UN/Clark et al. - 2022 - Risk assessment for marine ecosystem-based managem.pdf;/Users/davidbeauchesne/Zotero/storage/6T8B39CP/csp2.html}
}

@article{clarke2022,
  title = {Cumulative Effect Assessment in the Marine Environment: {{A}} Focus on the {{London}} Protocol/ {{London}} Convention},
  shorttitle = {Cumulative Effect Assessment in the Marine Environment},
  author = {Clarke, Charlotte and Lonsdale, Jemma-Anne and Judd, Adrian and Cormier, Roland and Martini, Natalia and Agius, Suzanne and Cavallaro, Kathleen and Oliver, James and Van Bloemestein, Ulric and family=Toit, given=Jessica, prefix=du, useprefix=true},
  date = {2022-10-01},
