@article{liversage2019,
  type = {Article},
  title = {Knowledge to Decision in Dynamic Seas: {{Methods}} to Incorporate Non-Indigenous Species into Cumulative Impact Assessments for Maritime Spatial Planning},
  author = {Liversage, Kiran and Kotta, Jonne and Aps, Robert and Fetissov, Mihhail and Nurkse, Kristiina and Orav-Kotta, Helen and Ratsep, Merli and Forsstrom, Tiia and Fowler, Amy and Lehtiniemi, Maiju and Normant-Saremba, Monika and Puntila-Dodd, Riikka and Arula, Timo and Hubel, Kalvi and Ojaveer, Henn},
  date = {2019-03-25},
  journaltitle = {SCIENCE OF THE TOTAL ENVIRONMENT},
  volume = {658},
  pages = {1452--1464},
  publisher = {ELSEVIER},
