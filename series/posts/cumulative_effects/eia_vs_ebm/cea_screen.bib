@article{mahon2019,
  type = {Article},
  title = {Additive and Interactive Cumulative Effects on Boreal Landbirds: Winners and Losers in a Multi-Stressor Landscape},
  author = {Mahon, C. Lisa and Holloway, Gillian L. and Bayne, Erin M. and Toms, Judith D.},
  date = {2019-07},
  journaltitle = {ECOLOGICAL APPLICATIONS},
  volume = {29},
