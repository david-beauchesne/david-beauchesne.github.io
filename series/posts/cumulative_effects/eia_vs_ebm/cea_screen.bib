  eissn = {1873-524X},
  funding-acknowledgement = {Flagship Project RITMARE - Italian Research for the Sea; Italian Ministry of Education, University and Research},
  funding-text = {This work was supported by the Flagship Project RITMARE - Italian Research for the Sea (www.ritmare.it), coordinated by the Italian National Research Council and funded by the Italian Ministry of Education, University and Research within the National Research Program 2012-2016. The authors would like to thank Fabio Grati, Luca Bolognini and Anna Nora Tassetti (CNR-IRBIM) for their support on data and analysis regarding fisheries, and Samantha Lorito and Lorenzo Calabrese (Emilia-Romagna Region) for their support on data collection and processing. Special thanks go to two anonymous reviewers for their suggestions and comments that contributed to the overall improvement of the manuscript.},
  journal-iso = {Ocean Coastal Manage.},
