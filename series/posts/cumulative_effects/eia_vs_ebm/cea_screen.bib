@article{popescu2020,
  type = {Article},
  title = {Quantifying Biodiversity Trade-Offs in the Face of Widespread Renewable and Unconventional Energy Development},
  author = {Popescu, Viorel D. and Munshaw, Robin G. and Shackelford, Nancy and Pouzols, Federico Montesino and Dubman, Evgenia and Gibeau, Pascale and Horne, Matt and Moilanen, Atte and Palen, Wendyi J.},
  date = {2020-05-05},
