  funding-text = {We would like to thank Arnout Gregoir, Charlotte Philippe, Tom Pinceel and two anonymous referees for their helpful comments on this manuscript. FB was supported by a EUROSA Erasmus Mundus action 2 PhD Scholarship.},
  journal-iso = {J. Nat. Conserv.},
  keywords-plus = {AREA RELATIONSHIPS; SPECIES RICHNESS; BIODIVERSITY; EXTINCTION; DISTRIBUTIONS; ALWAYS},
  langid = {english},
