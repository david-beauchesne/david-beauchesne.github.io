@article{sizo2016,
  type = {Article},
  title = {Strategic Environmental Assessment Framework for Landscape-Based, Temporal Analysis of Wetland Change in Urban Environments},
  author = {Sizo, Anton and Noble, Bram F. and Bell, Scott},
  date = {2016-03},
  journaltitle = {ENVIRONMENTAL MANAGEMENT},
