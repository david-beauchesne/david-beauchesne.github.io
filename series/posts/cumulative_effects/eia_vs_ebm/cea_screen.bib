@article{katsanevakis2020,
  type = {Review},
  title = {Twelve Recommendations for Advancing Marine Conservation in European and Contiguous Seas},
  author = {Katsanevakis, Stelios and Coll, Marta and Fraschetti, Simonetta and Giakoumi, Sylvaine and Goldsborough, David and Macic, Vesna and Mackelworth, Peter and Rilov, Gil and Stelzenmuller, Vanessa and Albano, Paolo G. and Bates, Amanda E. and Bevilacqua, Stanislao and Gissi, Elena and Hermoso, Virgilio and Mazaris, Antonios D. and Pita, Cristina and Rossi, Valentina and Teff-Seker, Yael and Yates, Katherine},
  date = {2020-10-22},
  journaltitle = {FRONTIERS IN MARINE SCIENCE},
  volume = {7},
  number = {565968},
  publisher = {FRONTIERS MEDIA SA},
  location = {AVENUE DU TRIBUNAL FEDERAL 34, LAUSANNE, CH-1015, SWITZERLAND},
  doi = {10.3389/fmars.2020.565968},
  abstract = {Like most ocean regions today, the European and contiguous seas experience cumulative impacts from local human activities and global pressures. They are largely in poor environmental condition with deteriorating trends. Despite several success stories, European policies for marine conservation fall short of being effective. Acknowledging the challenges for marine conservation, a 4-year multi-national network, MarCons, supported collaborative marine conservation efforts to bridge the gap between science, management and policy, aiming to contribute in reversing present negative trends. By consolidating a large network of more than 100 scientists from 26 countries, and conducting a series of workshops over 4 years (2016-2020), MarCons analyzed challenges, opportunities and obstacles for advancing marine conservation in the European and contiguous seas. Here, we synthesize the major issues that emerged from this analysis and make 12 key recommendations for policy makers, marine managers, and researchers. To increase the effectiveness of marine conservation planning, we recommend (1) designing coherent networks of marine protected areas (MPAs) in the framework of marine spatial planning (MSP) and applying systematic conservation planning principles, including re-evaluation of existing management zones, (2) designing MPA networks within a broader transboundary planning framework, and (3) implementing integrated land-freshwater-sea approaches. To address inadequate or poorly informed management, we recommend (4) developing and implementing adaptive management plans in all sites of the Natura 2000 European conservation network and revising the Natura 2000 framework, (5) embedding and implementing cumulative effects assessments into a risk management process and making them operational, and (6) promoting actions to reach `good environmental status' in all European waters. To account for global change in conservation planning and management, we further recommend (7) developing conservation strategies to address the impacts of global change, for example identifying climate-change refugia as high priority conservation areas, and (8) incorporating biological invasions in conservation plans and prioritizing management actions to control invasive species. Finally, to improve current practices that may compromise the effectiveness of conservation actions, we recommend (9) reinforcing the collection of high-quality open-access data, (10) improving mechanisms for public participation in MPA planning and management, (11) prioritizing conservation goals in full collaboration with stakeholders, and (12) addressing gender inequality in marine sciences and conservation.},
  affiliation = {Katsanevakis, S (Corresponding Author), Univ Aegean, Dept Marine Sci, Mitilini, Greece. Katsanevakis, Stelios, Univ Aegean, Dept Marine Sci, Mitilini, Greece. Coll, Marta, CSIC, Ecopath Int Initiat, ICM, Inst Marine Sci, Barcelona, Spain. Fraschetti, Simonetta, Univ Federico II Napoli, Stn Zool Anton Dohrn, Naples, Italy. Fraschetti, Simonetta; Bevilacqua, Stanislao, CoNISMa, Rome, Italy. Giakoumi, Sylvaine; Yates, Katherine, Univ Queensland, Ctr Biodivers \& Conservat Sci, Sch Biol Sci, Brisbane, Qld, Australia. Goldsborough, David, Univ Appl Sci, Van Hail Larenstein, Leeuwarden, Netherlands. Macic, Vesna, Univ Montenegro, Inst Marine Biol, Kotor, Montenegro. Mackelworth, Peter, Blue World Inst Marine Res \& Conservat, Veli Losinj, Croatia. Mackelworth, Peter, Inst Tourism, Zagreb, Croatia. Rilov, Gil, Natl Inst Oceanog, Israel Oceanog \& Limnol Res, Haifa, Israel. Stelzenmuller, Vanessa, Thunen Inst Sea Fisheries, Bremerhaven, Germany. Albano, Paolo G., Univ Vienna, Dept Paleontol, Vienna, Austria. Bates, Amanda E., Mem Univ Newfoundland, Dept Ocean Sci, St John, NF, Canada. Bevilacqua, Stanislao, Univ Trieste, Dept Life Sci, Trieste, Italy. Gissi, Elena, IUAV Univ Venice, Venice, Italy. Hermoso, Virgilio, Ctr Ciencia \& Tecnol Forestal Catalunya CTFC, Lleida, Spain. Mazaris, Antonios D., Aristotle Univ Thessaloniki, Dept Ecol, Sch Biol, Thessaloniki, Greece. Pita, Cristina, Univ Aveiro, CESAM Ctr Environm \& Marine Studies, Dept Environm \& Planning, Aveiro, Portugal. Rossi, Valentina, CNR, Inst Res Innovat \& Serv Dev, Naples, Italy. Teff-Seker, Yael, Technion Israel Inst Technol, Fac Architecture \& Town Planning, Haifa, Israel. Yates, Katherine, Univ Salford, Sch Environm \& Life Sci, Manchester, Lancs, England.},
