@article{ WOS:000568878800006,
Author = {Ho, Elaine and Trant, Andrew J. and Gray, Michelle A. and Courtenay,
   Simon C.},
Title = {Comparison of freshwater monitoring approaches: strengths,
   opportunities, and recommendations},
Journal = {ENVIRONMENTAL MONITORING AND ASSESSMENT},
Year = {2020},
Volume = {192},
Number = {9},
Month = {SEP 1},
Abstract = {This review identifies strengths and weaknesses of water monitoring
   programs selected by Canadian water managers. We used 22 criteria,
   guided by outcomes of an exploratory study and supported by 21
   semi-structured key informant interviews. The highest-scoring programs
   include the Slave Watershed Environmental Effects Program (Canada), the
   Government of Canada's Environmental Effects Monitoring Program, and
   Healthy Land and Water (Australia). We describe five recommendations for
   improving future freshwater monitoring frameworks: (1) recognize
   different knowledge approaches (especially Indigenous), (2) use multiple
   reporting formats, (3) clarify monitoring and management roles, (4)
   apply a whole-watershed approach, and (5) link monitoring to management
   and decision-making.},
