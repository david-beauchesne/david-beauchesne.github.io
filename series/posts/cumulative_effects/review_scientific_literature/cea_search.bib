@article{ WOS:000441853600026,
Author = {Coffin, Michael R. S. and Courtenay, Simon C. and Pater, Christina C.
   and van den Heuvel, Michael R.},
Title = {An empirical model using dissolved oxygen as an indicator for
   eutrophication at a regional scale},
Journal = {MARINE POLLUTION BULLETIN},
Year = {2018},
Volume = {133},
Pages = {261-270},
Month = {AUG},
Abstract = {Simple empirical models can sometimes capture salient patterns without
   sacrificing predictive capacity when compared to more complex models.
   Herein we examine dissolved oxygen as an indicator of eutrophication
   status for shallow estuaries. Dissolved oxygen was measured hourly in
   the upper estuary of 15 watersheds along a nutrient-loading and
   geographic gradient. Metrics describing hypoxia and supersaturation were
   devised and then analyzed using multivariate statistics, Results
   revealed independent responses for hypoxia and super saturation with
   hypoxia-related metrics correlating strongly with water residence. A
   metric integrating hypoxia and supersaturation effectively discriminated
   between seagrass and algae-dominated habitats and was significantly
   correlated with both water residence and nitrate-N loading. Chlorophyll,
   measured bi-weekly, was not correlated with our predictor variables
   likely because it does not account for benthic production. Over 70\% of
   the variability in hypoxia was explained by water residence and
   nitrate-N loading indicating that this model can be of use for managers.},
Publisher = {PERGAMON-ELSEVIER SCIENCE LTD},
Address = {THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Coffin, MRS (Corresponding Author), Univ Prince Edward Isl, Dept Fisheries \& Oceans, Moncton, NB E1C 9B6, Canada.
