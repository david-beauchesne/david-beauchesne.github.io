@article{ WOS:A1993MG57000002,
Author = {PARKER, S and COCKLIN, C},
Title = {THE USE OF GEOGRAPHICAL INFORMATION-SYSTEMS FOR CUMULATIVE
   ENVIRONMENTAL-EFFECTS ASSESSMENT},
