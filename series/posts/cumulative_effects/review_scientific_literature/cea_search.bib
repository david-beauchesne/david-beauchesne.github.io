@article{ WOS:000217330400007,
Author = {Muir, Bruce R. and Booth, Annie L.},
Title = {Losing Its Way: Environmental Impact Assessment in British Columbia,
   Canada},
Journal = {ENVIRONMENTAL JUSTICE},
Year = {2012},
Volume = {5},
Number = {3},
Pages = {164-167},
Month = {JUN},
Abstract = {Environmental impact assessment is viewed as an integral planning tool
   with respect to the sustainable development of land and natural
   resources, as it has the potential to protect the different values held
   by individuals and groups when done appropriately. This article examines
   an approach by the Environmental AssessmentOffice of British Columbia,
   Canada, regarding the scope of a cumulative effect assessment for the
   environmental assessment process of a proposed coal mine project that is
   endangering a threatened herd of caribou relied upon by West Moberly
   First Nations (an Indigenous group in Canada) for cultural subsistence.
   A Canadian-based equality framework is used to ground the environmental
   justice analysis. We conclude that the government's application of its
   discretionary powers in this case resulted the cultural values of
   WestMoberly being given a diminished level of protection and benefit of
   the law in comparison to the social values held by mainstream society.},
