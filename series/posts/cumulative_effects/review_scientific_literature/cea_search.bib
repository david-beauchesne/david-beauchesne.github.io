@article{ WOS:A1995QR83300012,
Author = {DIXON, J and MONTZ, BE},
Title = {FROM CONCEPT TO PRACTICE - IMPLEMENTING CUMULATIVE IMPACT ASSESSMENT IN
   NEW-ZEALAND},
