@inproceedings{ WOS:000344125500006,
Author = {Westhead, Maxine},
Editor = {Stephenson, RL and Annala, JH and Runge, JA and HallArber, M},
Title = {Tools for Integrated Policy and Management in the Gulf of Maine: A
   Summary of Major Advancements Since 1996},
