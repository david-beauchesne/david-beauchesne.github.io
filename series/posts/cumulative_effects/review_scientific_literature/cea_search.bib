@article{ WOS:000309805000002,
Author = {Williams, Pamela R. D. and Dotson, G. Scott and Maier, Andrew},
Title = {Cumulative Risk Assessment (CRA): Transforming the Way We Assess Health
   Risks},
Journal = {ENVIRONMENTAL SCIENCE \& TECHNOLOGY},
Year = {2012},
Volume = {46},
Number = {20},
Pages = {10868-10874},
Month = {OCT 16},
Abstract = {Human health risk assessments continue to evolve and now focus on the
   need for cumulative risk assessment (CRA). CRA involves assessing the
   combined risk from coexposure to multiple chemical and nonchemical
   stressors for varying health effects. CRAs are broader in scope than
   traditional chemical risk assessments because they allow for a more
   comprehensive evaluation of the interaction between different stressors
   and their combined impact on human health. Future directions of CRA
   include greater emphasis on local-level community-based assessments;
   integrating environmental, occupational, community, and individual risk
   factors; and identifying and implementing common frameworks and risk
   metrics for incorporating multiple stressors.},
